<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp_c__tiktok_player.MainPage">

    <StackLayout x:Name="Container">
        <WebView x:Name="MainWebView" HorizontalOptions="Fill" VerticalOptions="Fill" Source="https://www.tiktok.com/embed/7510888508546944261"/>
        <Button x:Name="StartButton" HeightRequest="100" Clicked="StartButton_Clicked" Text="Start" HorizontalOptions="Fill" VerticalOptions="Center" BackgroundColor="Black" FontAttributes="Bold" FontSize="Subtitle"/>
        <Button x:Name="NextButton" HeightRequest="100" BackgroundColor="Transparent" VerticalOptions="End" HorizontalOptions="Fill" Clicked="NextVideoButton_Clicked"/>
    </StackLayout>

</ContentPage>
